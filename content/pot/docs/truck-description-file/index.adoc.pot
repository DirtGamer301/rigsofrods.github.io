#: content/source/docs/truck-description-file/index.adoc:911
#, no-wrap
msgid "*start_function* [silver]#(optional)#: [red]#String# Inertia.\n"
msgstr ""

#. type: delimited block =
#: content/source/docs/truck-description-file/index.adoc:912
#, no-wrap
msgid "*stop_function* [silver]#(optional)#: [red]#String# Inertia.\n"
msgstr ""

